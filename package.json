{
  "name": "graphql-rxjs",
  "version": "0.10.5-0",
  "description": "Addon package for GraphQL which adds support for Observables",
  "contributors": [
    "Hagai Cohen <hagai.co@gmail.com>",
    "Lee Byron <lee@leebyron.com> (http://leebyron.com/)",
    "Nicholas Schrock <schrockn@fb.com>",
    "Daniel Schafer <dschafer@fb.com>"
  ],
  "license": "BSD-3-Clause",
  "main": "dist/bundle.js",
  "typings": "bundle.d.ts",
  "homepage": "https://github.com/DxCx/graphql-rxjs",
  "bugs": {
    "url": "https://github.com/DxCx/graphql-rxjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/DxCx/graphql-rxjs.git"
  },
  "scripts": {
    "postinstall": "node resources/fixModules.js",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "pretest": "npm run build",
    "test": "npm run testonly",
    "testonly": "jest",
    "prepublish": "npm run test"
  },
  "peerDependencies": {
    "graphql": "0.10.5",
    "rxjs": "^5.4.2"
  },
  "optionalDependencies": {
    "@types/graphql": "^0.10.1"
  },
  "devDependencies": {
    "@types/graphql": "^0.10.1",
    "@types/jest": "^20.0.4",
    "babel-plugin-detective": "^2.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "fs-extra": "^4.0.0",
    "graphql": "0.10.5",
    "graphql-tools": "^1.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "minimatch": "^3.0.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.2",
    "ts-jest": "^20.0.7",
    "typescript": "^2.4.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
